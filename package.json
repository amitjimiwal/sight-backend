{
  "name": "sightback",
  "version": "1.0.0",
  "description": "Backend server for TypeSight",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "start": "node dist/server.js",
    "dev": "nodemon ./src/server.ts",
    "format": "prettier --write \"src/**/*.ts\" ",
    "prepare": "husky",
    "migrate": "npx prisma migrate dev --name",
    "stripe:listen": "stripe listen --forward-to localhost:5555/api/v1/webhook",
    "stripe:trigger-payment": "stripe trigger payment_intent.succeeded"
  },
  "keywords": [],
  "author": "electroo096",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.13",
    "razorpay": "^2.9.2",
    "stripe": "^15.2.0",
    "zod": "^3.22.4",
    "husky": "^9.0.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.28",
    "@types/nodemailer": "^6.4.14",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
